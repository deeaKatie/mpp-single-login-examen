<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.PlayController">
   <children>
      <Label fx:id="usernameLabel" layoutX="14.0" layoutY="14.0" prefHeight="18.0" prefWidth="236.0" text="Hi, " />
      <Label fx:id="statusLabel" layoutX="263.0" layoutY="53.0" prefHeight="18.0" prefWidth="278.0" text="You won!" />
      <ListView fx:id="leftListView" layoutX="45.0" layoutY="155.0" prefHeight="200.0" prefWidth="200.0" />
      <Label fx:id="leftLabel" layoutX="117.0" layoutY="95.0" text="Games:" />
      <Label fx:id="rightLabel" layoutX="404.0" layoutY="95.0" text="Your data" />
      <Button fx:id="logOutButton" layoutX="534.0" layoutY="11.0" mnemonicParsing="false" onAction="#logOutHandler" text="Log out" />
      <Button layoutX="390.0" layoutY="330.0" mnemonicParsing="false" onAction="#makeAction" text="send answer" />
      <Label fx:id="questionLabel" layoutX="349.0" layoutY="182.0" text="Q: " />
      <TextField fx:id="reponseTextField" layoutX="357.0" layoutY="242.0" />
   </children>
</AnchorPane>
